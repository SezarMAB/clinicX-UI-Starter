{
  "name": "starter",
  "version": "0.0.0",
  "engines": {
    "yarn": ">=1.22.0 <2",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --watch",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "build:prod": "ng build --configuration production",
    "build:prod:stats": "ng build --configuration production --stats-json",
    "serve:prod": "npm run build:prod && node server.js",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"src/**/*.ts\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "hmr": "ng serve --hmr",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/starter/stats.json",
    "prepare": "is-ci || husky",
    "cap:sync": "npm run build:prod && npx cap sync",
    "cap:copy": "npm run build:prod && npx cap copy",
    "android:open": "npx cap open android",
    "android:run": "npx cap run android",
    "android:build": "npm run cap:sync && cd android && ./gradlew assembleDebug",
    "android:build:release": "npm run cap:sync && cd android && ./gradlew assembleRelease",
    "android:serve": "ng serve --host 0.0.0.0 --port 4200",
    "mobile:dev": "npm run android:serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.1",
    "@angular/cdk": "^20.1.1",
    "@angular/common": "^20.1.1",
    "@angular/compiler": "^20.1.1",
    "@angular/core": "^20.1.1",
    "@angular/forms": "^20.1.1",
    "@angular/material": "^20.1.1",
    "@angular/material-date-fns-adapter": "^20.1.1",
    "@angular/platform-browser": "^20.1.1",
    "@angular/platform-browser-dynamic": "^20.1.1",
    "@angular/router": "^20.1.1",
    "@capacitor/android": "^7.4.2",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/core": "^7.4.2",
    "@capacitor/filesystem": "^7.1.3",
    "@capacitor/network": "^7.0.1",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@ng-matero/extensions": "^20.2.0",
    "@ng-matero/extensions-date-fns-adapter": "^20.0.0",
    "@ngx-formly/core": "^7.0.0",
    "@ngx-formly/material": "^7.0.0",
    "@ngx-translate/core": "^16.0.0",
    "@ngx-translate/http-loader": "^16.0.0",
    "angular-in-memory-web-api": "~0.20.0",
    "apexcharts": "^5.2.0",
    "date-fns": "^4.1.0",
    "ng-matero": "^20.0.1",
    "ngx-permissions": "^19.0.0",
    "ngx-progressbar": "^14.0.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "screenfull": "^6.0.2",
    "tslib": "^2.6.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.0",
    "@angular/cli": "^20.1.0",
    "@angular/compiler-cli": "^20.1.1",
    "@capacitor/cli": "^7.4.2",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.0.14",
    "angular-eslint": "20.1.1",
    "compression": "^1.7.4",
    "eslint": "^8.57.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "husky": "^9.1.6",
    "is-ci": "^4.1.0",
    "jasmine-core": "~5.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.1.2",
    "parse5": "^8.0.0",
    "prettier": "^3.5.0",
    "protractor": "~7.0.0",
    "stylelint": "^16.16.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "ts-node": "~10.9.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0",
    "webpack-bundle-analyzer": "^4.10.0"
  }
}
